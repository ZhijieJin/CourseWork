0011 0000 0000 0000     ; x3000

0101 001 000 1 11111    ; R1 <- R0 & x0000
0101 010 000 1 11111    ; R2 <- R0 & x0000
0001 001 001 1 00000    ; R1 <- R1 + 0
0001 010 010 1 00000    ; R2 <- R2 + 0


0101 000 000 1 11111    ; R0 <- R0 & x0000
1001 010 010 111111     ; R2 NOT R2
0001 010 010 1 00001    ; R2 <- R2 + 1

0001 000 001 0 00 010   ; R0 <- R1 + R2
0000 100 000000010      ; BRz, x0002
0000 010 000000011      ; BRn, x0003
0000 001 000000100      ; BRp, x0004

0001 000 000 1 00000    ; R0 <- R0 + 0
0000 100 000000100      ; BRz, x0004

0001 000 000 1 11111    ; R0 <- R0 - 1
0000 010 000000010      ; BRn, x0002

0001 000 000 1 00001    ; R0 <- R0 + 1
0000 001 000000000      ; BRp, x0002

1111 0000 00100101      ; HALT

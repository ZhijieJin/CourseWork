; Start program at x3100
0011 0001 0000 0000     ; x3100
;
; Initialize R0 to 0
0101 000 000 1 00000    ; R0 <- R0 & x0000
;
; Negate R2 to -R2
1001 010 010 111111     ; R2 NOT R2
0001 010 010 1 00001    ; R2 <- R2 + 1
;
; Branch condition
0001 001 001 0 00 010   ; R1 <- R1 + R2
0000 010 000000101      ; BRz, x0005
0000 100 000000001      ; BRn, x0003
0000 001 000000010      ; BRp, x0002
;
; R1 < R2
0001 000 000 1 11111    ; R0 <- R0 - 1
0000 100 000000001      ; BRn, x0001
;
; R1 > R2
0001 000 000 1 00001    ; R0 <- R0 + 1
;
; Halt program
1111 0000 0010 0101     ; HALT
